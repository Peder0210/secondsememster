<script>
    function showMyPageInfo () {
        var url = window.location.pathname + window.location.search;
        var getQuery = url.split('?')[1];


        var params = getQuery.split('&');
     document.getElementById('navn').innerHTML = params[0].split('=')[1];
        document.getElementById('email').innerHTML = params[1].split('=')[1];
    }
</script>
